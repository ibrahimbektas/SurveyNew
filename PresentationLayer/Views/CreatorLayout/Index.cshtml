@{
    //CreatorLayout
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="au theme template">
    <meta name="author" content="Hau Nguyen">
    <meta name="keywords" content="au theme template">

    <!-- Title Page-->
    <title>Dashboard</title>

    @await Component.InvokeAsync("_CreatorLayoutCssPartial")

</head>

<body class="animsition">
    <div class="page-wrapper">
        <!-- HEADER MOBILE-->
        @await Component.InvokeAsync("_CreatorLayoutHeaderMobilePartial")
        <!-- END HEADER MOBILE-->
        <!-- MENU SIDEBAR-->
        @await Component.InvokeAsync("_CreatorLayoutMenuSidebarPartial")
        <!-- END MENU SIDEBAR-->
        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            @await Component.InvokeAsync("_CreatorLayoutHeaderDesktopPartial")
            <!-- HEADER DESKTOP-->
            <!-- MAIN CONTENT-->
            <div class="main-content">
                @RenderBody()
            </div>
            <!-- END MAIN CONTENT-->
            <!-- END PAGE CONTAINER-->
        </div>

    </div>

    @await Component.InvokeAsync("_CreatorLayoutScriptPartial")

</body>

</html>
<!-- end document-->
